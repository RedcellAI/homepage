{
  "mimeTypes": {
    ".html": "text/html"
  },
{
  "routes": [
    {
      "route": "/google*.html",
      "headers": {
        "Cache-Control": "no-store, must-revalidate"
      }
    },
    {
      "route": "/robots.txt",
      "headers": {
        "Cache-Control": "no-store, must-revalidate"
      }
    },
    {
      "route": "/sitemap.xml",
      "headers": {
        "Cache-Control": "no-store, must-revalidate"
      }
    }
  ]
}
